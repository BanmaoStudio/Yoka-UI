{
    "name": "youka-ui",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "commit": "git-cz"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "^16.2.4",
        "@commitlint/config-conventional": "^16.2.4",
        "@types/react": "^18.0.8",
        "@types/react-dom": "^18.0.3",
        "@umijs/fabric": "^2.10.2",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "typescript": "^4.6.4"
    },
    "lint-staged": {
        "src/**/*.ts?(x)": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ],
        "src/**/*.less": [
            "stylelint --syntax less --fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "prop-types": "^15.8.1"
    }
}
